<UserControl x:Class="LF.FictionWorld.Project.Controls.AttributeSelector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LF.FictionWorld.Project.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="200">
    <ListBox x:Name="ListAttributes" SelectionMode="Multiple" ItemContainerStyle="{StaticResource AttributeListItem}" DisplayMemberPath="Value"
             SelectionChanged="ListAttributes_SelectionChanged">
    </ListBox>
</UserControl>
