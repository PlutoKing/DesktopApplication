<?xml version="1.0" encoding="UTF-8"?>
<Book Code="310000001" Name="天乾造化诀">
  <Content>
    <Level ID="1" Name="淬体" Brief="感应天地之灵，淬炼肉体。" />
    <Level ID="2" Name="锻骨" Brief="吸收天地之灵，锻炼骨骼。" />
    <Level ID="3" Name="洗髓" Brief="运转天地之灵，洗涤骨髓，并转化为真灵。" />
    <Level ID="4" Name="神塔" Brief="真灵运转，开辟神塔。" />
    <Level ID="5" Name="结丹" Brief="凝聚真灵，凝结成丹。" />
    <Level ID="6" Name="造物" Brief="灵丹化物，结成道果。" />
    <Level ID="7" Name="铭道" Brief="在道果上铭刻道纹。" />
    <Level ID="8" Name="融道" Brief="将三魂七魄融入道果。" />
  </Content>
</Book>