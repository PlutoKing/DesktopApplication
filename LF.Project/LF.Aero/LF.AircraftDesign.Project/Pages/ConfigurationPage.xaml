<UserControl x:Class="LF.AircraftDesign.Project.Pages.ConfigurationPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LF.AircraftDesign.Project.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="3">
        <Grid.RowDefinitions>
            <RowDefinition Height="3*" />
            <RowDefinition Height="2*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
        </Grid.ColumnDefinitions>
        <GroupBox Header="主视图" Margin="3" Grid.Row="2"></GroupBox>
        <GroupBox Header="侧视图"  Grid.Column="1" Grid.Row="1" Margin="3"></GroupBox>
        <GroupBox Header="俯视图"  Grid.Column="1" Margin="3"  ></GroupBox>
        <GroupBox Header="布局选择" Margin="3"></GroupBox>
    </Grid>
</UserControl>
