<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- 主题色 -->
    <SolidColorBrush x:Key="MainColor" Color="#1D6BFF"/>

    <!-- 主题色附属 -->
    <SolidColorBrush x:Key="LightMainColor" Color="#AA1D6BFF" />

    <!-- 背景颜色 -->
    <SolidColorBrush x:Key="BackColor" Color="#F0F0F0" />
    
    <!-- 线框颜色 -->
    <SolidColorBrush x:Key="LineColor" Color="LightGray" />

    <SolidColorBrush x:Key="HighlightColor" Color="#FFAA2E"/>
    <SolidColorBrush x:Key="NoteColor" Color="#05CAAE"/>
    <SolidColorBrush x:Key="InfoColor" Color="#FF8282"/>
</ResourceDictionary>